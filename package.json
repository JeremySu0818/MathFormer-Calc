{
  "name": "mathformer-calc",
  "version": "1.0.0",
  "description": "A GUI calculator utilizing mathformer's four models for basic arithmetic and digit-based algorithms for addition, subtraction, multiplication, and division",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && tsc -p electron/tsconfig.json && electron .\"",
    "dev:ui": "vite --host",
    "package": "npm run build && electron-builder build --publish never",
    "build": "tsc -p electron/tsconfig.json && vite build",
    "build:electron": "tsc -p electron/tsconfig.json"
  },
  "keywords": [
    "electron",
    "calculator",
    "mathformer",
    "neural-network"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
